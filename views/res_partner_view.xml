    <record id="menu_crm_client" model="ir.ui.menu">
        <field name="name">Clients</field>
        <field name="sequence">10</field>
        <field name="parent_id" eval="ref('menu_crm_crm')"/>
        <field name="action"    ref="is_client_action"/>
        <field name="groups_id" eval="[(6,0,[
            ref('sales_team.group_sale_manager'),
        ])]"/>
    </record>





    <record id="is_view_res_partner_filter" model="ir.ui.view">
        <field name="name">is.res.partner.select</field>
        <field name="inherit_id" ref="base.view_res_partner_filter"/>
        <field name="model">res.partner</field>
        <field name="arch" type="xml">
            <field name="category_id" position="after">
                <field name="is_prenom"/>
                <field name="is_club_id"/>
                <field name="is_financeur_ids"/>
                <field name="is_type_contact"/>
                <field name="is_sport_id"/>
                <field name="is_niveau_id"/>
                <field name="is_statut_id"/>
                <field name="is_centre_formation"/>
                <field name="is_cellule_reconversion"/>
                <field name="is_statut_sportif_id"/>
                <field name="is_referencement"/>
                <field name="is_agrement"/>
                <field name="is_region_id"/>
                <field name="city"/>
                <field name="is_createur_id" />
                <field name="is_alerte_activite" />
            </field>
            <filter name="type_person" position="before">
                <filter string="Sans club" name="sans_club" domain="[('is_club_id','=',False)]"/>
            </filter>
            <filter name="salesperson" position="before">
                <filter name="club"      string="Club" context="{'group_by': 'is_club_id'}"/>
                <filter name="apporteur" string="Apporteur d'affaire" context="{'group_by': 'is_apporteur_affaire_id'}"/>
                <filter name="referent"  string="Référent" context="{'group_by': 'is_referent_id'}"/>
                <filter name="createur"  string="Créateur" context="{'group_by': 'is_createur_id'}"/>
            </filter>
            <group name="group_by" position="after">
                <group string="Afficher les champs">
                    <filter name="agrement" string="Agrément" context="{'invisible_is_agrement': False}" />
                </group>
            </group>
        </field>
    </record>


