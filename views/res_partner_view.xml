<?xml version="1.0" encoding="utf-8"?>

<odoo>
    <data>

        <menuitem name="CRM"       id="menu_crm"           sequence="1"/>
        <menuitem name="CRM"       id="menu_crm_crm"       sequence="10"  parent="menu_crm"/>
        <menuitem name="Documents" id="menu_crm_documents" sequence="20"  parent="menu_crm" groups="sales_team.group_sale_manager"/>



        <record id="calculer_suivi_activite_actions_server" model="ir.actions.server">
            <field name="name">Calculer suivi activité</field>
            <field name="model_id" ref="base.model_res_partner"/>
            <field name="binding_model_id" ref="base.model_res_partner"/>
            <field name="binding_view_types">form,list</field>
            <field name="state">code</field>
            <field name="code">
    action = model._calculer_suivi_activite_action()
            </field>
        </record>



        <!-- Menu Sportifs -->
        <record model="ir.ui.view" id="is_sportif_tree_view">
            <field name="name">is_sportif_tree_view</field>
            <field name="model">res.partner</field>
            <field name="arch" type="xml">
                <tree>
                    <field name="name"/>
                    <field name="is_prenom"/>
                    <field name="phone"/>
                    <field name="mobile"/>
                    <field name="email"/>
                    <field name="is_club_id"/>
                    <field name="is_sport_id"/>
                    <field name="is_date_naissance"/>
                    <field name="is_statut_sportif_id"/>
                    <field name="is_agrement"     invisible="context.get('invisible_is_agrement', True)"/>
                    <field name="is_financeur_ids"/>
                    <field name="is_type_contact"/>
                </tree>
            </field>
        </record>
        <record id="is_sportif_action" model="ir.actions.act_window">
            <field name="name">Sportifs</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">res.partner</field>
            <field name="view_id" ref="is_sportif_tree_view"/>
            <field name="context">{}</field>
            <field name="domain">[('is_type_contact','=','sportif')]</field>
            <field name="view_mode">tree,kanban,form</field>
        </record>
        <record id="is_sportif_action2" model="ir.actions.act_window">
            <field name="name">Sportifs</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">res.partner</field>
            <field name="view_id" ref="is_sportif_tree_view"/>
            <field name="context">{'default_is_club_id': active_id, 'search_default_is_club_id': active_id}</field>
            <field name="domain">[('is_type_contact','=','sportif')]</field>
            <field name="view_mode">tree,kanban,form</field>
        </record>
        <record id="is_sportif_action3" model="ir.actions.act_window">
            <field name="name">Sportifs</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">res.partner</field>
            <field name="view_id" ref="is_sportif_tree_view"/>
            <field name="context">{'default_is_financeur_ids': active_id, 'search_default_is_financeur_ids': active_id}</field>
            <field name="domain">["|",('is_type_contact','=','club'),('is_type_contact','=','sportif')]</field>
            <field name="view_mode">tree,kanban,form</field>
        </record>
        <menuitem action="is_sportif_action" id="menu_crm_sportif" parent="menu_crm_crm" sequence="30" groups="is_group_sportif"/>

        <record model="ir.ui.view" id="is_view_partner_form">
            <field name="name">is_view_partner_form</field>
            <field name="model">res.partner</field>
            <field name="inherit_id" ref="base.view_partner_form"/>
            <field name="arch" type="xml">
                <xpath expr="//div[@name='button_box']/button[1]" position="before">
                   <button class="oe_inline oe_stat_button" type="action"
                            attrs="{'invisible': [('is_type_contact', '!=', 'club')]}"
                            name="%(is_nouvelle_trajectoire.is_sportif_action2)d"
                            icon="fa-users">
                        <field string="Sportifs" name="is_sportif_count" widget="statinfo"/>
                    </button>
                    <button 
                        class="oe_inline oe_stat_button" type="action"
                        attrs="{'invisible': [('is_type_contact', '!=', 'financeur_institutionnel'),
                                              ('is_type_contact', '!=', 'financeur_prive')]}"
                        name="%(is_nouvelle_trajectoire.is_sportif_action3)d"
                        icon="fa-users"
                        groups="is_nouvelle_trajectoire.is_group_club"
                    >
                        <field string="Club/Sportif" name="is_club_sportif_count" widget="statinfo"/>
                    </button>

                    <button class="oe_inline oe_stat_button" type="action"
                            attrs="{'invisible': False}"
                            name="%(is_nouvelle_trajectoire.is_suivi_activite_action2)d"
                            icon="fa-users">
                        <field string="Suivi d'activité" name="is_suivi_activite_count" widget="statinfo"/>
                    </button>
                </xpath>
               <field name="name" position="before">
                    <field name="is_prenom" placeholder="Prénom" string="Prénom" attrs="{'invisible': [('is_company','=', True)]}"/>
                </field>
               <field name="website" position="after">
                    <field name="is_facebook"/>
                </field>
               <field name="email" position="replace">
                    <field name="email"/>
                    <field name="is_type_contact"/>
                </field>
                <!-- <field name="state_id" position="replace">
                    <field name="state_id" options='{"no_open": True}' on_change="onchange_state(state_id)" attrs="{'invisible':True, 'readonly': [('use_parent_address','=',True)]}"/>
                </field> -->
                <!-- <field name="title" position="replace">
                    <field name="title" 
                           options='{"no_open": True}' 
                           attrs="{'invisible': [('is_type_contact','!=', 'sportif'),('is_type_contact','!=', 'ambassadeur'),('is_type_contact','!=', 'messager')]}"/>
                </field> -->
                <field name="country_id" position="before">
                    <field name="is_region_id" placeholder="Région" />
                </field>
                <field name="user_id" position="replace">
                    <field string="Titulaire" name="user_id" context="{}" />
                </field>

                <xpath expr="//sheet/group/group/field[@name='type']" position="before">
                    <field name="is_type_contact"/>
                    <field name="is_referent_id"/>
                </xpath>
                <xpath expr="//sheet/notebook/page[@name='contact_addresses']" position="before">
                    <page string="CRM" autofocus="autofocus">
                        <group>
                            <group string="Sportif" colspan="4" attrs="{'invisible': [('is_type_contact','!=', 'sportif')]}">
                                <field name="is_club_id" domain="[('is_type_contact','=','club')]" />
                                <field name="is_syndicat_id" domain="[('is_type_contact','=','syndicat')]" />
                                <field name="is_dernieres_saisons"             />
                                <field name="is_date_naissance"                />
                                <field name="is_statut_sportif_id"             />
                                <field name="is_situation_familiale"/>
                                <field name="is_niveau_etude"/>
                                <field name="is_situation_contractuelle"/>
                                <field name="is_remuneration_annuelle"/>
                                <field name="is_raison_fin_carriere"/>
                                <field name="is_projet"/>
                                <field name="is_nationalite_id"/>
                            </group>
                            <group string="Club" colspan="4" attrs="{'invisible': [('is_type_contact','!=', 'club')]}">
                                <field name="is_niveau_id"                    />
                                <field name="is_opca1"                    />
                                <field name="is_opca2"                    />

                                <field name="is_budget_annuel"                />
                                <field name="is_budget_formation"             />
                                <field name="is_centre_formation"             />
                                <field name="is_cellule_reconversion"         />
                                <field name="reseaux_sociaux"                 />
                                <field name="is_nature_besoins"               />
                            </group>
                            <group string="Financeur" colspan="4" attrs="{'invisible': [('is_type_contact','!=', 'financeur_institutionnel'),('is_type_contact','!=', 'financeur_prive')]}">
                                <field name="is_referencement"/>
                                <field name="is_agrement"/>
                                <field name="is_activite"/>
                                <field name="is_ca"                      attrs="{'invisible': [('is_type_contact','!=', 'financeur_prive')]}"/>
                                <field name="is_taxe_apprentissage"      attrs="{'invisible': [('is_type_contact','!=', 'financeur_prive')]}"/>
                                <field name="is_prise_en_charge_sportif" attrs="{'invisible': [('is_type_contact','!=', 'financeur_prive')]}"/>
                            </group>
                            <group string="Ambassadeur" colspan="4" attrs="{'invisible': [('is_type_contact','!=', 'ambassadeur')]}">
                                <field name="is_contrat_nt"/>
                                <field name="is_date_fin_contrat"/>
                                <field name="is_couverture_geographique"/>
                                <field name="is_nb_contacts"/>
                                <field name="is_objectif_contact"/>
                                <field name="is_objectif_contrat"/>
                            </group>
                            <group string="Messager" colspan="4" attrs="{'invisible': [('is_type_contact','!=', 'messager')]}">
                                <field name="is_rattachement_nt"/>
                                <field name="is_prochaine_action"/>
                                <field name="is_type_action"/>
                            </group>
                            <group string="Autres informations" colspan="4">
                                <field name="is_financeur_ids" 
                                       widget="many2many_tags"
                                       domain="['|',('is_type_contact','=','financeur_institutionnel'),('is_type_contact','=','financeur_prive')]" 
                                       attrs="{'invisible': [('is_type_contact','!=', 'club'),('is_type_contact','!=', 'sportif')]}"/>
                                <field name="is_apporteur_affaire_id" 
                                       domain="['|',('is_type_contact','=','ambassadeur'),('is_type_contact','=','messager')]" 
                                       attrs="{'invisible': [('is_type_contact','!=', 'club'),('is_type_contact','!=', 'sportif')]}"/>
                                <field name="is_date_qualification" />
                                <field name="is_nb_salaries" attrs="{'invisible': [('is_company','!=', True)]}"/>
                                <field name="is_sport_id"  
                                       attrs="{'invisible': [('is_type_contact','!=', 'club'),
                                                             ('is_type_contact','!=', 'sportif'),
                                                             ('is_type_contact','!=', 'ambassadeur'),
                                                             ('is_type_contact','!=', 'syndicat'),
                                                             ('is_type_contact','!=', 'federation_ligue')]}"/>
                                <field name="is_federation_id"  
                                       attrs="{'invisible': [('is_type_contact','!=', 'club'),
                                                             ('is_type_contact','!=', 'sportif')]}"
                                       domain="[('is_type_contact','=','federation_ligue')]" />
                                <field name="is_statut_id"  
                                       attrs="{'invisible': [('is_type_contact','!=', 'sportif'),
                                                             ('is_type_contact','!=', 'messager')]}"/>
                                <field name="is_historique_nt" 
                                       attrs="{'invisible': [('is_type_contact','!=', 'federation_ligue'),
                                                             ('is_type_contact','!=', 'syndicat'),
                                                             ('is_type_contact','!=', 'ambassadeur'),
                                                             ('is_type_contact','!=', 'messager')]}"/>
                                <field name="is_informations_complementaires" />
                                <field name="is_siret" />
                                <field name="is_createur_id" readonly="1" />
                            </group>
                        </group>
                    </page>
                    <page string="Suivi d'activité">
                        <group groups="sales_team.group_sale_manager">
                            <group>
                                <field name="is_budget_previsionnel" />
                                <field name="is_total_facture" />
                                <field name="is_ecart_budget" />
                            </group>
                            <group>
                                <field name="is_nb_activite_en_cours"/>
                                <field name="is_nb_activite_terminee"/>
                                <field name="is_date_derniere_activite"/>
                                <field 
                                    name="is_alerte_activite"
                                    style="background-color:orange;font-size:11pt" 
                                    attrs="{'invisible': [('is_alerte_activite','=', False)],'readonly':True}"
                                />
                            </group>
                        </group>
                        <group>
                            <field name="is_suivi_activite_ids" nolabel="1">
                                <tree editable="bottom">
                                    <field name="createur_id"/>
                                    <field name="type_activite_id"/>
                                    <field name="date_prevue"/>
                                    <field name="date_realisee"/>
                                    <field name="commentaire"/>
                                </tree>
                            </field>
                        </group>
                    </page>
                </xpath>
            </field>
        </record>   

    <!-- Menu Clients -->
    <record model="ir.ui.view" id="is_client_tree_view">
