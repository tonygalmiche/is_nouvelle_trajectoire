            <tree>
                <field name="name"/>
                <field name="is_prenom"/>
                <field name="is_type_contact"/>
                <field name="is_group_id" optional="hide"/>
                <field name="parent_id"   optional="hide"/>
                <field name="is_sport_id"/>
                <field name="is_referent_id"/>
                <field name="phone"/>
                <field name="mobile"/>
                <field name="email"/>
                <field name="is_createur_id" />
            </tree>
        </field>
