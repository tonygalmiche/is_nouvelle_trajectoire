   <!-- Menu Entreprises -->
    <record model="ir.ui.view" id="is_entreprise_tree_view">
        <field name="name">is_entreprise_tree_view</field>
        <field name="model">res.partner</field>
        <field name="arch" type="xml">
            <tree>
                <field name="name"/>
                <field name="is_prenom"/>
                <field name="phone"/>
                <field name="mobile"/>
                <field name="email"/>
                <field name="is_historique_nt"/>
            </tree>
        </field>
    </record>
    <record id="is_entreprise_action" model="ir.actions.act_window">
        <field name="name">Entreprises</field>
        <field name="type">ir.actions.act_window</field>
        <field name="res_model">res.partner</field>
        <field name="view_id" ref="is_entreprise_tree_view"/>
        <field name="domain">[('is_type_contact','=','entreprise')]</field>
        <field name="view_mode">tree,kanban,form</field>
    </record>
    <menuitem action="is_entreprise_action" id="menu_crm_messager" parent="menu_crm_crm" sequence="50"  groups="is_group_entreprise"/>



    <!-- Suivi du budget -->
    <record id="is_suivi_budget_pivot" model="ir.ui.view">
        <field name="name">is_suivi_budget_pivot</field>
        <field name="model">res.partner</field>
        <field name="arch" type="xml">
            <pivot>
                <field name="is_referent_id"         type="row"/>
                <field name="is_type_contact"        type="col"/>
                <field name="is_budget_previsionnel" type="measure"/>
            </pivot>
        </field>
    </record>
    <record id="is_suivi_budget_graph" model="ir.ui.view">
        <field name="name">is_suivi_budget_graph</field>
        <field name="model">res.partner</field>
        <field name="arch" type="xml">
            <graph type="bar" orientation="vertical" stacked="False">
                <field name="is_referent_id"        type="row"/>
                <field name="is_budget_previsionnel" type="measure"/>
            </graph>
        </field>
    </record>
    <record model="ir.ui.view" id="is_suivi_budget_tree_view">
        <field name="name">is_suivi_budget_tree_view</field>
        <field name="model">res.partner</field>
        <field name="arch" type="xml">
            <tree>
                <field name="name"/>
                <field name="is_prenom"/>
                <field name="is_type_contact"/>
                <field name="is_referent_id"/>
                <field name="is_budget_previsionnel"  sum="Total"/>
                <field name="is_total_facture"        sum="Total"/>
                <field name="is_ecart_budget"         sum="Total"/>
                <field name="is_nb_activite_en_cours" sum="Total"/>
                <field name="is_nb_activite_terminee" sum="Total"/>
                <field name="is_date_derniere_activite"/>
                <field name="is_alerte_activite"/>
            </tree>
        </field>
    </record>
    <record id="is_suivi_budget_action" model="ir.actions.act_window">
        <field name="name">Suivi du budget</field>
        <field name="type">ir.actions.act_window</field>
        <field name="res_model">res.partner</field>
        <field name="view_id" ref="is_suivi_budget_tree_view"/>
        <field name="domain">[('is_budget_previsionnel','>',0)]</field>
        <field name="view_mode">tree,kanban,form,graph,pivot</field>
    </record>




